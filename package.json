{
  "name": "www.kamesuta.com-monorepo",
  "version": "0.0.0",
  "repository": "https://github.com/KamePowerWorld/www.kamesuta.com",
  "private": true,
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "scripts": {
    "build": "turbo run build",
    "build:minify": "turbo run build && turbo run build:minify",
    "clean:build": "turbo run clean:build",
    "clean:install": "turbo run clean:install",
    "clean:turbo": "shx rm -rf pnpm-lock.yaml node_modules",
    "dev": "turbo run dev",
    "fix": "turbo run fix",
    "install:next": "turbo run install:next"
  },
  "devDependencies": {
    "turbo": "^1.6.3",
    "shx": "^0.3.4"
  }
}
